[PaStA]
### Project name. Should not contain whitespaces ###
PROJECT_NAME = Linux
MODE = mbox

### necessary for upstream comparison. Patches within this range will be considered ###
UPSTREAM = origin/master

### Blacklist of upstream commit hashes to ignore for upstream comparison ###
UPSTREAM_BLACKLIST = linux-blacklist

### Project specific autothresholds for interactive rating ###
INTERACTIVE_THRESHOLD = 1.1
AUTOACCEPT_THRESHOLD = 0.82
DIFF_LINES_RATIO = 0.4
MESSAGE_DIFF_WEIGHT = 0.3

### Used for parsing mailinglists ###
[MBOX]
MBOX_MINDATE = 2005-04-16
MBOX_MAXDATE = 2030-01-01

[MBOX_RAW]
# listname = filename
# ...

[MBOX_GIT_PUBLIC_INBOX]
# A mailing list that uses public inboxes may consist of several public
# inboxes. Those inboxes are specified in a comma separated list:
#
# listname = pub-inbox-1
# listname = pub-inbox-1, pub-inbox-2[, ...]
#
# Those inboxes must be locates at
# resources/project_name/resources/mbox/pubin/pub-inbox-X and should be added
# as git submodules of the PaStA-resources repositories.
#
# Nevertheless, absolute paths are supported as well:
#
# listname = /some/location/pub-inbox-0[, ...]


